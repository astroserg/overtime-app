<li class="<%= active?(audit_logs_path) %>">
  <%= link_to "Audit Log", audit_logs_path %>
</li>

<tr>
  <td>
    <%= audit_log.id %>
  </td>
  <td>
    <%= audit_log.user.full_name %>
  </td>
  <td>
    <%= audit_log.start_date %>
  </td>
  <td>
    <%= audit_log.end_date %>
  </td>
  <td>
    <%= audit_log.status %>
  </td>
</tr>